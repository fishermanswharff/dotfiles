export PATH="$PATH:$HOME/.rvm/gems/ruby-2.4.3/bin:$HOME/.rvm/gems/ruby-2.5.3/bin:$HOME/.rvm/bin/rvm" # Add RVM to PATH for scripting
export PATH=/usr/bin:/bin:/usr/sbin:/sbin:/usr/local/bin:/usr/X11/bin
export PATH=$PATH:/usr/local/bin:/usr/local/sbin:~/node_modules/.bin:/usr/local/share/npm/bin:/usr/local/Library/Aliases:${PATH}
export PATH=$PATH:/Applications/Postgres.app/Contents/Versions/latest/bin
export PATH=/usr/local/bin/mysql:$PATH
export PATH=/usr/local/mysql/bin:$PATH
launchctl setenv PATH $PATH
export DYLD_LIBRARY_PATH=/usr/local/mysql/lib:$DYLD_LIBRARY_PATH
export PATH=./bin:/usr/local/bin:$PATH
export PATH=/usr/local/bin/python3:/usr/bin/python:/usr/local/bin/python2:$PATH
export PATH="$HOME/bin:$PATH";
export PATH="$HOME/.chefdk/gem/ruby/2.5.0/bin:$PATH"
# gradle
if [[ -d /opt/gradle ]]; then
  export PATH="/opt/gradle/gradle-5.6.4/bin:$PATH"
fi
